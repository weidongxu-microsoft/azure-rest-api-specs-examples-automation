{
  "sdkExample": {
    "repository": "https://github.com/Azure/azure-rest-api-specs-examples"
  },
  "sdkConfigurations": [
    {
      "name": "azure-sdk-for-net",
      "language": "dotnet",
      "repository": "https://github.com/Azure/azure-sdk-for-net",
      "releaseTag": {
        "regexMatch": "^Azure.ResourceManager.*_.+$",
        "packageRegexGroup": "(.*)_.*",
        "versionRegexGroup": ".*_(.*)"
      },
      "script": {
        "run": "dotnet/main.sh"
      }
    }
  ]
}
