{
  "sdkExample": {
    "repository": "https://github.com/weidongxu-microsoft/azure-rest-api-specs-examples"
  },
  "sdkConfigurations": [
    {
      "name": "azure-sdk-for-java",
      "language": "java",
      "repository": "https://github.com/Azure/azure-sdk-for-java",
      "releaseTag": {
        "regexMatch": "azure-resourcemanager.*_.+",
        "packageRegexGroup": "(.*)_.*",
        "versionRegexGroup": ".*_(.*)"
      },
      "script": {
        "run": "java/main.sh"
      }
    }
  ]
}
