{
  "sdkExample": {
    "repository": "https://github.com/Azure/azure-rest-api-specs-examples"
  },
  "sdkConfigurations": [
    {
      "name": "azure-sdk-for-java",
      "language": "java",
      "repository": "https://github.com/Azure/azure-sdk-for-java",
      "releaseTag": {
        "regexMatch": "^(?!azure-resourcemanager.*_.+-hybrid)azure-resourcemanager.*_.+$",
        "packageRegexGroup": "(.*)_.*",
        "versionRegexGroup": ".*_(.*)"
      },
      "script": {
        "run": "java/main.sh"
      }
    },
    {
      "name": "azure-sdk-for-go",
      "language": "go",
      "repository": "https://github.com/Azure/azure-sdk-for-go",
      "releaseTag": {
        "regexMatch": "^sdk/resourcemanager/.*$",
        "packageRegexGroup": "(.*)/.*",
        "versionRegexGroup": ".*/(.*)"
      },
      "script": {
        "run": "go/main.sh"
      }
    },
    {
      "name": "azure-sdk-for-js",
      "language": "js",
      "repository": "https://github.com/Azure/azure-sdk-for-js",
      "releaseTag": {
        "regexMatch": "^@azure\/arm-.*$|^@azure-rest\/arm-.*$",
        "packageRegexGroup": "(.*)_.*",
        "versionRegexGroup": ".*_(.*)"
      },
      "script": {
        "run": "js/main.sh"
      }
    },
    {
      "name": "azure-sdk-for-net",
      "language": "dotnet",
      "repository": "https://github.com/Azure/azure-sdk-for-net",
      "releaseTag": {
        "regexMatch": "^Azure.ResourceManager.*_.+$",
        "packageRegexGroup": "(.*)_.*",
        "versionRegexGroup": ".*_(.*)"
      },
      "script": {
        "run": "dotnet/main.sh"
      }
    },
    {
      "name": "azure-sdk-for-python",
      "language": "python",
      "repository": "https://github.com/Azure/azure-sdk-for-python",
      "releaseTag": {
        "regexMatch": "^azure-mgmt-.*_.+$",
        "packageRegexGroup": "(.*)_.*",
        "versionRegexGroup": ".*_(.*)"
      },
      "script": {
        "run": "python/main.sh"
      }
    }
  ]
}
